    <script>
      //knob
      $(function() {
        $(".knob").knob({
          'draw': function() {
            $(this.i).val(this.cv + '%')
          }
        })
      });

      //carousel
      $(document).ready(function() {
        $("#owl-slider").owlCarousel({
          navigation: true,
          slideSpeed: 300,
          paginationSpeed: 400,
          singleItem: true

        });
      });

      //custom select box

      $(function() {
        $('select.styled').customSelect();
      });

      /* ---------- Map ---------- */
      $(function() {
        $('#map').vectorMap({
          map: 'world_mill_en',
          series: {
            regions: [{
              values: gdpData,
              scale: ['#000', '#000'],
              normalizeFunction: 'polynomial'
            }]
          },
          backgroundColor: '#eef3f7',
          onLabelShow: function(e, el, code) {
            el.html(el.html() + ' (GDP - ' + gdpData[code] + ')');
          }
        });
      });
      // console.log(status)
      $.ajax({
        url:"/updateserver",
        method:"GET",
        cache:false,
        contentType:'application/json',
        dataType:'json',
        success:data=>{
            if (data.status===2){
document.getElementById('yan').style.display="none";
document.getElementById("no").innerHTML="Обновлено"
document.getElementById('no1').style.display="none"
      }
      if (data.status===0){
          document.getElementById('no').style.display="none"
          document.getElementById('yan').style.display="none";
document.getElementById("no1").innerHTML="Интернета Не Существует!"
      }
        }
    })
      </script>
